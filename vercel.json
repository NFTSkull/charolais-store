{
  "version": 2,
  "rewrites": [
    {
      "source": "/config.js",
      "destination": "/api/config"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/(.*\\.(js|css|html|png|jpg|jpeg|gif|svg|ico|JPG))",
      "destination": "/public/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
} 